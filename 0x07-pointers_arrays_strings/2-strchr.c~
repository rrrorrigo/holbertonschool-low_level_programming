#include "holberton.h"
/**
 * _strchr - function that copies memory area
 * @s: destiny character
 * @c: source character
 *
 * Return: the first occurence of the character c in the string s, or null.
 */
char *_strchr(char *s, char c)
{
	int contador = 0;

	while ((*s != '\0') && *s != c))
		s++;
	return (s);
}
