#include "holberton.h"
#define NULL ((void *)0)
/**
 * _strpbrk - search a string or any of a set of bytes.
 * @s: external value
 * @accept: external value
 * Return: s
 */
char *_strpbrk(char *s, char *accept)
{
	int i = 0;
	while (accept[i] != '\0')
	{
		if ((*s != '\0') && *s != accept[i])
		{
			s++;
		}
		else

	}
	if (accept[i] == *s)
		return (s);
		return (NULL);
}
